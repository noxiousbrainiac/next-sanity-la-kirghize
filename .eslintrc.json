{
  "extends": [
    "next/core-web-vitals",
    "airbnb",
    "airbnb-typescript"
  ],
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "rules": {
    "import/no-extraneous-dependencies": 0,
    "import/prefer-default-export": "off",
    "no-param-reassign": "off",
    "max-len": "off",
    "react/jsx-props-no-spreading": "off",
    "no-plusplus": "off"
  }
}
